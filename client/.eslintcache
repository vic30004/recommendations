r\nimport UserReducer from \"./UserReducer\";\r\n\r\nconst UserState = (props) => {\r\n  const initialState = {\r\n    user: null,\r\n    isAuthenticated: false,\r\n    loading: true,\r\n    error: [],\r\n  };\r\n\r\n  const [state, dispatch] = useReducer(UserReducer, initialState);\r\n\r\n  const { data, loading, error } = useQuery(LOADUSER);\r\n\r\n  const handleError = () => {\r\n    dispatch({\r\n      type: USER_ERROR,\r\n      payload: error,\r\n    });\r\n  };\r\n\r\n\r\n  const loadUser = () => {\r\n    dispatch({\r\n      type: LOGIN,\r\n      payload: data,\r\n    });\r\n  };\r\n\r\n  useEffect(() => {\r\n    if (localStorage[\"token\"] !== \"\") {\r\n      loadUser();\r\n    }\r\n  }, [loading, localStorage[\"token\"]]);\r\n\r\n  useEffect(()=>{\r\n    if(error){\r\n      handleError()\r\n    }\r\n  },[loading])\r\n  return (\r\n    <UserContext.Provider\r\n      value={{\r\n        loadUser,\r\n        user: state.user,\r\n        userLoading: state.loading,\r\n        isAuthenticated: state.isAuthenticated,\r\n      }}\r\n    >\r\n      {props.children}\r\n    </UserContext.Provider>\r\n  );\r\n};\r\nexport default UserState;\r\n","/app/src/components/common/Modal.js",[],"/app/src/components/common/ModalTrigger.js",[],"/app/src/graphql/Users.js",[],"/app/src/context/types.js",[],"/app/src/context/User/UserContext.js",[],"/app/src/components/Items/Content.js",["335"],"import React, { useContext } from \"react\";\r\nimport UserContext from \"../../context/User/UserContext\";\r\nimport { ItemContentContainer } from \"../../styles/Items\";\r\nimport Cards from \"../common/Cards\";\r\nimport { generateCard } from \"../common/helper/GenerateCards\";\r\n\r\nconst Content = ({\r\n  loading,\r\n  data,\r\n  error,\r\n  deleteItem,\r\n  editItem,\r\n  editLoading,\r\n}) => {\r\n  const userContext = useContext(UserContext);\r\n  const { user } = userContext;\r\n\r\n  if (loading) return \"Loading...\";\r\n  if (error) {\r\n    console.log(error);\r\n  }\r\n\r\n  