ispatch] = useReducer(UserReducer, initialState);\r\n\r\n  const { data, loading, error } = useQuery(LOADUSER);\r\n\r\n  const handleError = () => {\r\n    dispatch({\r\n      type: USER_ERROR,\r\n      payload: error,\r\n    });\r\n  };\r\n\r\n\r\n  const loadUser = () => {\r\n    dispatch({\r\n      type: LOGIN,\r\n      payload: data,\r\n    });\r\n  };\r\n\r\n  useEffect(() => {\r\n    if (localStorage[\"token\"] !== \"\") {\r\n      loadUser();\r\n    }\r\n  }, [loading, localStorage[\"token\"]]);\r\n\r\n  useEffect(()=>{\r\n    if(error){\r\n      handleError()\r\n    }\r\n  },[loading])\r\n  return (\r\n    <UserContext.Provider\r\n      value={{\r\n        loadUser,\r\n        user: state.user,\r\n        userLoading: state.loading,\r\n        isAuthenticated: state.isAuthenticated,\r\n      }}\r\n    >\r\n      {props.children}\r\n    </UserContext.Provider>\r\n  );\r\n};\r\nexport default UserState;\r\n","/app/src/components/common/Modal.js",[],"/app/src/components/common/ModalTrigger.js",[],"/app/src/graphql/Users.js",[],"/app/src/context/types.js",[],"/app/src/context/User/UserContext.js",[],"/app/src/components/Items/Content.js",[],"/app/src/context/User/UserReducer.js",["329","330","331"],"import { LOGIN, REGISTER_USER, USER_ERROR } from \"../types\";\r\nimport { LOADUSER } from \"../../graphql/Users\";\r\n\r\nexport default (state, action) => {\r\n  const { type, payload } = action;\r\n  switch (type) {\r\n    case LOGIN:\r\n      return {\r\n        ...state,\r\n        isAuthenticated: true,\r\n        loading: false,\r\n        user: payload,\r\n      };\r\n    case USER_ERROR:\r\n      return {\r\n        ...state,\r\n        loading: false,\r\n        error: [payload],\r\n      };\r\n    default:\r\n      return state;\r\n  }\r\n};\r\n","/app/src/components/Items/Header.js",["332","333","334"],"import React, { useState, useEffect } from \"react\";\r\nimport { Image, Placeholder } from \"cloudinary-react\";\r\nimport {\r\n  ItemHeaderContainer,\r\