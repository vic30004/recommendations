    handleError()\r\n    }\r\n  },[loading])\r\n  return (\r\n    <UserContext.Provider\r\n      value={{\r\n        loadUser,\r\n        user: state.user,\r\n        userLoading: state.loading,\r\n        isAuthenticated: state.isAuthenticated,\r\n      }}\r\n    >\r\n      {props.children}\r\n    </UserContext.Provider>\r\n  );\r\n};\r\nexport default UserState;\r\n",["300","301"],"/app/src/components/common/Modal.js",[],"/app/src/components/common/ModalTrigger.js",[],"/app/src/graphql/Users.js",[],"/app/src/context/types.js",[],"/app/src/context/User/UserContext.js",[],"/app/src/components/Items/Content.js",[],"/app/src/context/User/UserReducer.js",["302","303","304"],"/app/src/components/Items/Header.js",["305","306","307"],"/app/src/components/hooks/useToggle.js",["308"],"/app/src/components/hooks/UseForm.js",["309"],"/app/src/components/Recommendations/AddContainer.js",["310","311","312","313"],"/app/src/components/Recommendations/AddItems.js",["314"],"/app/src/components/utils/CloudinaryWidget.js",[],"/app/src/components/Profile/Header.js",["315","316","317","318"],"/app/src/components/Recommendations/index.js",[],"/app/src/styles/Recommendations/Recommendations.js",[],"/app/src/graphql/index.js",[],"/app/src/components/common/ModalStyles/ModalStyles.js",[],"/app/src/components/Homepage/index.js",[],"/app/src/styles/Homepage/index.js",[],"/app/src/styles/Items/index.js",["319"],"/app/src/components/common/Cards.js",["320","321","322","323"],"import React, { Fragment, useEffect, useState } from \"react\";\r\nimport { Image, Placeholder } from \"cloudinary-react\";\r\n\r\nimport { Card, CardTitle, TitleContainer } from \"./CardStyles/CardStyles\";\r\nimport Modal from \"./Modal\";\r\nimport useToggle from \"../hooks/useToggle\";\r\nimport useForm from \"../hooks/UseForm\";\r\nimport AddItems from \"../Recommendations/AddItems\";\r\nimport { openWidget } from \"../utils/CloudinaryWidget\";\r\nimport { Link } from \"react-router-dom\";\r\nconst Cards = ({\r\n  pi