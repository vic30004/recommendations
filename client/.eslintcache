\n  const randomNumGen = (len) => {\r\n    return Math.floor(Math.random() * len);\r\n  };\r\n\r\n  return (\r\n    <>\r\n      <ItemHeaderContainer>\r\n        <HeaderContentContainer>\r\n          {recommendationData ? (\r\n            <Image\r\n              cloud_name='dawyijhjw'\r\n              publicId={\r\n                recommendationData.getRecommendationsByUsername[\r\n                  randomNumGen(\r\n                    recommendationData.getRecommendationsByUsername.length\r\n                  )\r\n                ].main_picture || \"recommendation/dbnrnrpv0paeg7unzgjh\"\r\n              }\r\n              width='auto'\r\n              crop='fill'\r\n              quality='auto'\r\n              responsive\r\n              responsiveUseBreakpoints='true'\r\n              loading='lazy'\r\n              format='webp'\r\n              flag='prgressive'\r\n              effect='blur:120'\r\n              height='350'\r\n            >\r\n              <Placeholder type='pixelate' />\r\n            </Image>\r\n          ) : (\r\n            <Image\r\n              cloud_name='dawyijhjw'\r\n              publicId='recommendation/dbnrnrpv0paeg7unzgjh'\r\n              width='auto'\r\n              crop='fill'\r\n              quality='auto'\r\n              responsive\r\n              responsiveUseBreakpoints='true'\r\n              loading='lazy'\r\n              format='webp'\r\n              flag='prgressive'\r\n              effect='blur:120'\r\n              height='350'\r\n            >\r\n              <Placeholder type='pixelate' />\r\n            </Image>\r\n          )}\r\n          <HeaderInfoContainer>\r\n            <h1>Welcome to {user[0].username} profile </h1>\r\n            <FlexRow>\r\n              {recommendationData ? (\r\n                <p>\r\n                  {makePlural(\r\n                    recommendationData.getRecommendationsByUsername.length\r\n                  )}\r\n                  : {recommendationData.getRecommendationsByUsername.length}\r\n                </p>\r\n              ) : (\r\n                <p>Recommendation:0 </p>\r\n              )}\r\n              {followData ? (\r\n                <p>Following: {followData.getFollowsByUserId.length}</p>\r\n              ) : (\r\n                <p>Following: 0 </p>\r\n              )}\r\n            </FlexRow>\r\n          </HeaderInfoContainer>\r\n        </HeaderContentContainer>\r\n      </ItemHeaderContainer>\r\n      <>\r\n        {recommendationData ? (\r\n          <ProfileBody\r\n            recommendations={recommendationData.getRecommendationsByUsername}\r\n            query={GET_RECOMMENDATION_BY_USER_ID}\r\n            user_id={user_id}\r\n          />\r\n        ) : (\r\n          <>\r\n            <h2>Recommendations</h2> <p>No Recommendations yet</p>\r\n          </>\r\n        )}\r\n        {followData ? (\r\n          <ProfileBody\r\n            recommendations={followData.getFollowsByUserId}\r\n            follow={true}\r\n          />\r\n        ) : (\r\n          <ItemContentContainer>\r\n            <FlexColumn>\r\n              <h2>Follows</h2> <p>Not Following Anything</p>\r\n            </FlexColumn>\r\n          </ItemContentContainer>\r\n        )}\r\n      </>\r\n    </>\r\n  );\r\n};\r\n\r\nexport default Header;\r\n","/app/src/components/Recommendations/index.js",[],"/app/src/styles/Recommendations/Recommendations.js",[],"/app/src/graphql/index.js",[],"/app/src/components/common/ModalStyles/ModalStyles.js",[],"/app/src/components/Homepage/index.js",[],"/app/src/styles/Homepage/index.js",[],"/app/src/styles/Items/index.js",["345"],"import { from } from '@apollo/client';\r\n\r\nexport * from './ItemsContainer';\r\nexport * from './ItemContnet'","/app/src/components/common/Cards.js",["346","347","348","349","350","351"],"/app/src/styles/Items/ItemsContainer.js",[],"/app/src/components/Items/RecommendationsCotnent.js",["352","353","354","355"],"import React, { useContext } from \"react\";\r\nimport { useQuery } from \"@apollo/client\";\r\nimport { GET_USER } from \"../../graphql\";\r\nimport { Link } from \"react-router-dom\";\r\nconst RecommendationsCotnent = ({ id, category, follow }) => {\r\n  const { data, loading, error, refetch } = useQuery(GET_USER, {\r\n    variables: { id },\r\n    suspend: false,\r\n  });\r\n\r\n  return (\r\n    <>\r\n      <li>Category: {category}</li>\r\n      {data ? (\r\n        <Link to={`/${data.user.username}`}>\r\n          <li>Author:{data.user.username}</li>\r\n        </Link>\r\n      ) : (\r\n        \"\"\r\n      )}\r\n\r\n      <li>Follows: {follow < 0 ? 0 : follow}</li>\r\n    </>\r\n  );\r\n};\r\n\r\nexport default RecommendationsCotnent;\r\n","/app/src/components/Recommendations/AddRecommendation.js",["356"],"import React, {Fragment} from 'react'\r\nimport { AddWrapper } from \"../../styles/Recommendations\";\r\nimport { Button, Form, InputBuilder } from \"../common\";\r\n\r\nconst AddRecommendation = ({toggle,toggleItems,title,handleChange,category,description,main_picture,setFormData, openWidget,mutationLoading}) => {\r\n    return (\r\n        <AddWrapper>\r\n        <div className='top-container'>\r\n          <h3 className='close-btn' onClick={toggle}>\r\n            <i class='fas fa-times-circle'></i>\r\n          </h3>\r\n          <h2>New Recommendation</h2>\r\n        </div>\r\n  \r\n        <Form>\r\n          <InputBuilder\r\n            type='text'\r\n            name='title'\r\n            placeholder='Title'\r\n            value={title}\r\n            setFormData={handleChange}\r\n          />\r\n          <InputBuilder\r\n            type='text'\r\n            name='category'\r\n            placeholder='Category'\r\n            value={category}\r\n            setFormData={handleChange}\r\n            textArea={false}\r\n          />\r\n          <InputBuilder\r\n            name='description'\r\n            id='description'\r\n            placeholder='Description'\r\n            value={description}\r\n            textArea={true}\r\n            setFormData={handleChange}\r\n          ></InputBuilder>\r\n  \r\n          <Button\r\n            center\r\n            marg={\"0\"}\r\n            size={\"70%\"}\r\n            name=\"main_picture\"\r\n            backg={main_picture ? \"rgba(146, 204, 194, 1)\" : \"\"}\r\n            onClick={e=>openWidget(e,setFormData)}\r\n          >\r\n            {main_picture ? \"Upload Complete\" : \"Upload Photo\"}\r\n          </Button>\r\n  \r\n          <Button\r\n            center\r\n            size={\"70%\"}\r\n            backg={\"rgba(221, 114, 145, 1)\"}\r\n            shadowC={\"#333\"}\r\n            onClick={toggleItems}\r\n          >\r\n            {mutationLoading ? \"loading...\" : \"Next\"}\r\n          </Button>\r\n        </Form>\r\n      </AddWrapper>\r\n    )\r\n}\r\n\r\nexport default AddRecommendation\r\n","/app/src/components/Recommendations/Controls.js",["357","358","359"],"/app/src/components/Profile/ProfileBody.js",["360","361","362","363","364","365"],"/app/src/components/Recommendations/UpdatesSections.js",["366"],"import React from \"react\";\r\nimport { GET_RECOMMENDATIONS } from \"../../graphql/\";\r\nimport { UpdateContainerBuilder } from \"../common/UpdateContainerBuilder\";\r\n\r\nimport { useQuery } from \"@apollo/client\";\r\n\r\nexport const UpdatesSections = () => {\r\n  const { data, loading, error, fetchMore } = useQuery(GET_RECOMMENDATIONS);\r\n\r\n  if (loading) return \"Loading...\";\r\n\r\n  if (error) return \"Error\";\r\n\r\n  const returnFiveRecentRecommendation = (data) => {\r\n    if (data.length < 5) {\r\n      return data;\r\n    }\r\n    const res = data.slice(0, 5);\r\n    return res;\r\n  };\r\n\r\n\r\n  return (\r\n    <UpdateContainerBuilder\r\n      title={\"Recent Recommendations\"}\r\n      data={returnFiveRecentRecommendation(data.recommendations)}\r\n    />\r\n  );\r\n};\r\n","/app/src/components/Recommendations/AddSection.js",[],"/app/src/components/Recommendations/Recomme